{
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"type": "parserhook",
	"MessagesDirs": {
		"Example": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"LoreKeeper": "LoreKeeper.body.php",
		"Calendar": "metadata/Calendar.php",
		"InPageCalendar": "metadata/InPageCalendar.php",
		"LKDate": "metadata/LKDate.php",
		"Event": "metadata/Event.php",
		"Timeline": "metadata/Timeline.php"
	},
	"ExtensionMessagesFiles": {
		"LoreKeeper": "LoreKeeper.i18n.php"
	},
	"Hooks": {
		"ParserFirstCallInit": "LoreKeeper::onParserFirstCallInit"
	},
	"config": {
		"LoreKeeperCalendarPage": {
			"value": "Calendars"
		}	
	},
	"manifest_version": 2
}
